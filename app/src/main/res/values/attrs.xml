<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProgressWheel">
        <attr name="countText"      format="string"/>
        <attr name="definitionText" format="string"/>
        <attr name="progressColor"  format="color"/>
        <attr name="rimColor"       format="color"/>
        <attr name="countTextColor" format="color"/>
        <attr name="defTextColor"   format="color"/>
        <attr name="barWidth"       format="dimension"/>
        <attr name="countTextSize"  format="dimension"/>
        <attr name="defTextSize"    format="dimension"/>
        <attr name="marginBtwText"  format="dimension"/>
        <attr name="percentage"    format="integer"/>
    </declare-styleable>

    <declare-styleable name="DayButton">
        <attr name="dayOfMonth"       format="reference|integer"/>
        <attr name="dotColor"   format="reference|color"/>
    </declare-styleable>
</resources>